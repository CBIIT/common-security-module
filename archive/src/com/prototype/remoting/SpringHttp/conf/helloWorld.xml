<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="helloWorldService" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean">
        <property name="serviceUrl">
            <value>http://localhost:8080/SpringHttp12/http/helloWorld</value>
        </property>
        <property name="serviceInterface">
            <value>com.prototype.remoting.SpringHttp.HelloWorld</value>
        </property>
    </bean>
</beans>
