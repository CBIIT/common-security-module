<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gov.nih.nci.security.ri.valueObject.Project" 
    table="project"
>

    <id
        name="projectId"
        type="java.lang.Long"
        column="PROJECT_ID"
    >
        <generator class="identity" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        length="50"
    />
    <property
        name="description"
        type="java.lang.String"
        column="DESCRIPTION"
        length="200"
    />

    <!-- Associations -->
  
    

</class>
</hibernate-mapping>
