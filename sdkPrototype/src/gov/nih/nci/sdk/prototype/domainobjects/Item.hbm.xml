<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>


<class 
    name="gov.nih.nci.sdk.prototype.domainobjects.Item" 
    table="Item"
>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
        unsaved-value="null"
    >
        <generator class="identity"/>
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        not-null="true"
        length="100"
    />



</class>
</hibernate-mapping>

