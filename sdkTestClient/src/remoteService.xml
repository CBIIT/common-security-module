<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="remoteService" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean">
    <property name="serviceUrl">
      <value>http://localhost:8080/sdkremoting/http/remoteService</value>
    </property>
    <property name="serviceInterface">
      <value>com.codegen.application.common.RemoteSDKApplicationService</value>
    </property>
  </bean>
</beans>

