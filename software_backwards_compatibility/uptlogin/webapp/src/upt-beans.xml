<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="false" default-dependency-check="none" default-autowire="no">
 
	<bean name="FileHelper" class="gov.nih.nci.security.loginapp.util.properties.FileHelper" singleton="true" />

	<bean name="UPTProperties" class="gov.nih.nci.security.loginapp.util.properties.UPTProperties" singleton="true">
		<constructor-arg ref="FileHelper"></constructor-arg>
		<constructor-arg value="ApplicationSecurityConfig.xsd"></constructor-arg>
	</bean>

</beans>