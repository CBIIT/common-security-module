<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gov.nih.nci.security.authorization.domainobjects.ProtectionElement" 
    table="protection_element"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="protection_element"
    </meta>

    <id
        name="protectionElementId"
        type="java.lang.Long"
        column="PROTECTION_ELEMENT_ID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Long"
            column="PROTECTION_ELEMENT_ID"

        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="protectionElementName"
        type="java.lang.String"
        column="PROTECTION_ELEMENT_NAME"
        not-null="true"
        length="100"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="PROTECTION_ELEMENT_NAME"
            length="100"
            not-null="true"
        </meta>    
    </property>
    <property
        name="protectionElementDescription"
        type="java.lang.String"
        column="PROTECTION_ELEMENT_DESCRIPTION"
        length="200"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="PROTECTION_ELEMENT_DESCRIPTION"
            length="200"
        </meta>    
    </property>
    <property
        name="objectId"
        type="java.lang.String"
        column="OBJECT_ID"
        not-null="true"
        length="100"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="OBJECT_ID"
            length="100"
            not-null="true"
        </meta>    
    </property>
    <property
        name="attribute"
        type="java.lang.String"
        column="ATTRIBUTE"
        length="100"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="ATTRIBUTE"
            length="100"
        </meta>    
    </property>
    <property
        name="protectionElementTypeId"
        type="long"
        column="PROTECTION_ELEMENT_TYPE_ID"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="PROTECTION_ELEMENT_TYPE_ID"
            length="10"
            not-null="true"
        </meta>    
    </property>
    <property
        name="updateDate"
        type="java.sql.Date"
        column="UPDATE_DATE"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="UPDATE_DATE"
            length="10"
            not-null="true"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to UserProtectionElement -->
    <set
        name="userProtectionElements"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="PROTECTION_ELEMENT_ID"

           @hibernate.collection-one-to-many
            class="gov.nih.nci.security.authorization.domainobjects.UserProtectionElement"
        </meta>
        <key>
            <column name="PROTECTION_ELEMENT_ID" />
        </key>
        <one-to-many 
            class="gov.nih.nci.security.authorization.domainobjects.UserProtectionElement"
        />
    </set>
    <!-- bi-directional one-to-many association to ProtectionGroupProtectionElement -->
    <set
        name="protectionGroupProtectionElements"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="PROTECTION_ELEMENT_ID"

           @hibernate.collection-one-to-many
            class="gov.nih.nci.security.authorization.domainobjects.ProtectionGroupProtectionElement"
        </meta>
        <key>
            <column name="PROTECTION_ELEMENT_ID" />
        </key>
        <one-to-many 
            class="gov.nih.nci.security.authorization.domainobjects.ProtectionGroupProtectionElement"
        />
    </set>
    <!-- bi-directional many-to-one association to Application -->
    <many-to-one
        name="application"
        class="gov.nih.nci.security.authorization.domainobjects.Application"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="APPLICATION_ID"         
        </meta>
        <column name="APPLICATION_ID" />
    </many-to-one>

</class>
</hibernate-mapping>
